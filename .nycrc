{
    "check-coverage": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 95,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/experiment/**/*.ts",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "report-dir": "./coverage",
    "all": true
}