{
  "name": "alpha",
  "version": "0.0.1",
  "description": "alpha",
  "directories": {
    "test": "test"
  },
  "license": "MIT",
  "repository": "",
  "author": "Camel King",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/alpha.js",
  "typings": "typings/alpha.d.ts",
  "scripts": {
    "test": "PORT=8000 nodemon --ext ts --watch ./  --exec 'npm run mocha' --delay 1",
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "echo Using TypeScript && tsc --version && tsc --pretty",
    "watch:build": "npm run build -- --watch",
    "mocha": "echo Using Mocha && mocha --version && mocha --compilers ts:ts-node/register --recursive 'test/**/*.spec.ts' --reporter spec --slow 0",
    "watch:mocha": "PORT=8000 nodemon --ext ts --quiet --watch ./  --exec 'npm run mocha' --delay 1",
    "nyc": "echo Using Istanbul/nyc && nyc --version && nyc  mocha --compilers ts:ts-node/register  --recursive 'test/**/*.spec.ts'",
    "watch:nyc": "PORT=8000 nodemon --ext ts  --watch ./  --exec 'npm run nyc' --delay 1",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "make": "npm run lint && npm run build && npm run mocha",
    "forensic": "sh -c 'node --version; npm --version; npm ls' > output.txt"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/chai-as-promised": "0.0.30",
    "@types/mocha": "^2.0.0",
    "@types/node": "^6.0.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.12.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^2.1.2",
    "tslint": "^4.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "lib",
      "node_modules",
      "typings",
      "coverage",
      "test",
      ".sandbox",
      ".nyc_output",
      "coverage"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./coverage",
    "all": true
  }
}