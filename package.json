{
  "name": "alpha",
  "version": "0.0.1",
  "description": "alpha",
  "directories": {
    "test": "test"
  },
  "license": "MIT",
  "repository": "",
  "author": "Camel King",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/alpha.js",
  "typings": "typings/alpha.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "nyc --reporter=text --reporter=html mocha -r ts-node/register  ./test/**/*.spec.ts  --recursive  --reporter spec  --slow 0",
    "testmon": "PORT=8000 nodemon --ext ts --watch ./  --exec 'mocha -r ts-node/register  ./test/**/*.spec.ts  --recursive  --reporter spec  --slow 0' --delay 1",
    "testonce": "mocha --compilers ts:ts-node/register --recursive 'test/**/*.spec.ts' --reporter spec --slow 0",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register  --recursive 'test/**/*.spec.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "build:test": "npm run build && npm run testonce",
    "watch:coverage": "npm run coverage -- --watch",
    "forensic": "sh -c 'node --version; npm --version; npm ls' > output.txt"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/chai-as-promised": "0.0.30",
    "@types/mocha": "^2.0.0",
    "@types/node": "^6.0.0",
    "chai": "^3.0.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^2.1.2",
    "tslint": "^4.0.0",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "lib",
      "node_modules",
      "typings",
      "coverage",
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}